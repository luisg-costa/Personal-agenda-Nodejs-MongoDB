<% if (success.length > 0) {%>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success">
                <%= success %>
            </div>
        </div>
    </div>
<% } %>

<% if (error.length > 0) {%>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-danger">
                <%= error %>
            </div>
        </div>
    </div>
<% } %>